{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --no-cache --max-warnings=60",
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "node dist/app.js",
    "dev": "env-cmd -f environments/dev.env nodemon src/app.ts",
    "dev:prod": "env-cmd -f environments/prod.env nodemon src/app.ts",
    "build:dev": "env-cmd -f environments/dev.env npm run lint && tsc -p .",
    "build": "env-cmd -f environments/prod.env npm run lint && tsc -p .",
    "build:nolint": "env-cmd -f environments/dev.env tsc -p .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
